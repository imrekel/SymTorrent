#include <aiftool.rh>

RESOURCE AIF_DATA
    {
    app_uid=0x2000804B;
    num_icons=2;
    embeddability=KAppEmbeddable;
    newfile=KAppDoesNotSupportNewFile;

    datatype_list=
    {
        DATATYPE { priority=EDataTypePriorityHigh; type="application/x-symtorrent"; }
    };

    }

