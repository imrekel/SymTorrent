#include <appinfo.rh>
#include <SymTorrent.rsg>

UID2 KUidAppRegistrationResourceFile
UID3 0xA0001751 // application UID
RESOURCE APP_REGISTRATION_INFO
	{
	app_file="SymTorrent"; // filename of application binary (minus extension)

	localisable_resource_file = "\\resource\\apps\\SymTorrent.rsc";
	localisable_resource_id = R_LAI;

	embeddability = KAppNotEmbeddable;

	datatype_list= 
		{
		DATATYPE { priority=EDataTypePriorityHigh; type="application/x-symtorrent"; }
		};
	}
